  "Serilog": {
    "LogLevel": {
      "Using": [],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information",
          "System": "Warning"
        }
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "./data/log.csv",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:G};{Level:u3};{Message};{NewLine}{Exception}"
        }
      }
    ]
  },
